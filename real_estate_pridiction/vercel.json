{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/predict_home_price",
      "dest": "server.py"
    },
    {
      "src": "/api/get_location_names",
      "dest": "server.py"
    },
    {
      "src": "/(.*)",  // Default route to catch everything else
      "dest": "server.py"
    }
  ]
}
